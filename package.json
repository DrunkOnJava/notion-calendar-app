{
  "name": "notion-calendar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:verbose": "NODE_OPTIONS='--max-old-space-size=8192' NEXT_TELEMETRY_DEBUG=1 next build --debug",
    "build:analyze": "ANALYZE=true NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:profile": "NODE_OPTIONS='--max-old-space-size=8192' next build --profile",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' next dev",
    "dev:verbose": "NODE_OPTIONS='--max-old-space-size=8192' NEXT_TELEMETRY_DEBUG=1 next dev --debug",
    "dev:turbo": "NODE_OPTIONS='--max-old-space-size=8192' next dev --turbo",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:verbose": "eslint . --format=verbose --max-warnings=0",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "type-check:verbose": "tsc --noEmit --listFiles --explainFiles",
    "type-check:trace": "tsc --noEmit --diagnostics --extendedDiagnostics",
    "build:full-logs": "bash scripts/build-with-logs.sh",
    "test:env:up": "bash scripts/test-env-up.sh",
    "test:env:down": "bash scripts/test-env-down.sh",
    "test:env:clean": "bash scripts/test-env-down.sh --clean",
    "test:env:restart": "pnpm test:env:down && pnpm test:env:up",
    "test:env:status": "bash scripts/test-env-status.sh",
    "test:env:logs": "docker compose -f docker-compose.test.yml logs -f",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:e2e:install": "playwright install --with-deps",
    "copilot:chrome:screenshot": "pnpm dlx chrome-devtools-mcp@latest --channel stable --headless true --isolated true --viewport 1920x1080 --chromeArg=--hide-scrollbars",
    "copilot:chrome:audit": "pnpm dlx chrome-devtools-mcp@latest --channel stable --headless true --isolated true --viewport 1280x720 --chromeArg=--enable-logging --chromeArg=--ignore-certificate-errors",
    "copilot:chrome:inspect": "pnpm dlx chrome-devtools-mcp@latest --channel stable --viewport 1440x900",
    "copilot:chrome:debug": "pnpm dlx chrome-devtools-mcp@latest --channel dev --chromeArg=--auto-open-devtools-for-tabs --chromeArg=--remote-debugging-port=9222",
    "copilot:chrome:evaluate": "pnpm dlx chrome-devtools-mcp@latest --channel canary --headless true --isolated true --no-category-emulation --chromeArg=--disable-gpu"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.8",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-menubar": "1.1.16",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-toggle-group": "1.1.11",
    "@radix-ui/react-tooltip": "1.2.8",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "lucide-react": "^0.553.0",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-day-picker": "9.11.1",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "3.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.39.1",
    "testcontainers": "^11.7.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
